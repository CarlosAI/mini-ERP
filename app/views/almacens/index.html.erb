<p id="notice"><%= notice %></p>

<h1>Almacens</h1>

<table>
  <thead>
    <tr>
      <th>Id almacen</th>
      <th>Nombre almacen</th>
      <th>Descripcion</th>
      <th>Direccion</th>
      <th>Codigo postal</th>
      <th>Ciudad</th>
      <th>Estado</th>
      <th>Pais</th>
      <th>Cls</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @almacens.each do |almacen| %>
      <tr>
        <td><%= almacen.id_almacen %></td>
        <td><%= almacen.nombre_almacen %></td>
        <td><%= almacen.descripcion %></td>
        <td><%= almacen.direccion %></td>
        <td><%= almacen.codigo_postal %></td>
        <td><%= almacen.ciudad %></td>
        <td><%= almacen.estado %></td>
        <td><%= almacen.pais %></td>
        <td><%= almacen.cls %></td>
        <td><%= link_to 'Show', almacen %></td>
        <td><%= link_to 'Edit', edit_almacen_path(almacen) %></td>
        <td><%= link_to 'Destroy', almacen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Almacen', new_almacen_path %>
